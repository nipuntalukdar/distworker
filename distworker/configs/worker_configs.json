{
  "heartbeat_interval": 10,
  "max_tasks_in_queue": 16,
  "max_task_pull": 1,
  "redis_host": "127.0.0.1",
  "redis_port": 6379,
  "task_streams": {
    "tasks1": {
      "maxlen": 100
    },
    "tasks2": {
      "maxlen": 100
    },
    "tasks3": {
      "maxlen": 100
    }
  },
  "taskgroup": "taskgroup",
  "min_idle_time_pending": 120000,
  "max_wait_milli_second": 10000,
  "min_idle_time_pending_c_disappeared": 1200000,
  "redis_ssl": true,
  "ssl_ca_certs": "/usr/share/distworker/tls/ca.crt",
  "ssl_certfile": "/usr/share/distworker/tls/client.crt",
  "ssl_keyfile": "/usr/share/distworker/tls/client.key"
}
